generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Earnings {
  id                   Int         @id @default(autoincrement())
  symbol               String
  companyName          String
  reportDate           String
  estimatedEps         String?
  lastYearReportDate   String?
  time                 String
  lastYearEps          String?
  fiscalQuarterEnding  String?
  marketCap            String?
  numberOfEstimates    String?
  createdAt            DateTime    @default(now())
  updatedAt            DateTime    @updatedAt

  @@unique([symbol, reportDate], name: "symbol_reportDate")
  @@index([reportDate])
}